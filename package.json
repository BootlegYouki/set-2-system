{
  "name": "set-2-system",
  "private": true,
  "version": "0.0.1",
  "description": "SET-2 System Desktop App",
  "author": "Youki",
  "type": "module",
  "engines": {
    "node": "22"
  },
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "start": "node build",
    "generate-icons": "node scripts/generate-pwa-icons.js",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on tcp:5173 && electron .\"",
    "electron:start": "cross-env NODE_ENV=production electron .",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/adapter-node": "^5.3.1",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/mcp": "^0.1.11",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "png-to-ico": "^3.0.1",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "sharp": "^0.34.5",
    "svelte": "^5.0.0",
    "vite": "^7.0.4",
    "vite-plugin-pwa": "^1.1.0",
    "wait-on": "^9.0.3"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "bcrypt": "^6.0.0",
    "chart.js": "^4.5.1",
    "countup.js": "^2.9.0",
    "dotenv": "^17.2.2",
    "electron-squirrel-startup": "^1.0.1",
    "mongodb": "^6.20.0",
    "node-fetch": "^3.3.2",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "xlsx": "^0.18.5"
  }
}
